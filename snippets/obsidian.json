{
    "Example": {
      "prefix": "bsp",
      "body": "> [!example]-\n> "
    },
    "Abstract": {
      "prefix": "summary",
      "body": "> [!abstract]+\n> "
    },
    "Definition": {
      "prefix": "def",
      "body": "> [!important]+ Definition $1\n> $2"
    },
    "Remark": {
      "prefix": "rem",
      "body": "> [!info]- Bemerkung\n> "
    },
    "Warning": {
      "prefix": "warning",
      "body": "> [!warning]-\n> "
    },
    "Quote": {
      "prefix": "quote",
      "body": "> [!quote]-\n> "
    },
    "Theorem": {
      "prefix": "satz",
      "body": "> [!abstract]- $1\n> $2"
    }
}
